From a6b6d421f3a890079971a9e9559f31535448e950 Mon Sep 17 00:00:00 2001
From: Felix Hammarstrand <felix.hammarstrand@flir.se>
Date: Fri, 17 Aug 2018 17:06:05 +0200
Subject: [PATCH] bblc: disable rpmsg

---
 arch/arm/boot/dts/imx7ulp-bblc.dts | 8 ++++----
 1 file changed, 4 insertions(+), 4 deletions(-)

diff --git a/arch/arm/boot/dts/imx7ulp-bblc.dts b/arch/arm/boot/dts/imx7ulp-bblc.dts
index b70f67878e96..d824198aea54 100644
--- a/arch/arm/boot/dts/imx7ulp-bblc.dts
+++ b/arch/arm/boot/dts/imx7ulp-bblc.dts
@@ -61,7 +61,7 @@
 		#interrupt-cells = <2>;
 		interrupt-controller;
 		interrupt-parent = <&rpmsg_gpio0>;
-		status = "okay";
+		status = "disabled";
 	};
 
 	rpmsg_gpio1: rpmsg-gpio1 {
@@ -72,12 +72,12 @@
 		#interrupt-cells = <2>;
 		interrupt-controller;
 		interrupt-parent = <&rpmsg_gpio1>;
-		status = "okay";
+		status = "disabled";
 	};
 
 	v4l2_cap_0 {
 		compatible = "fsl,imx7-rpmsg-capture";
- 		status = "okay";
+ 		status = "disabled";
 	};
 };
 
@@ -231,7 +231,7 @@
 	 */
 	vdev-nums = <2>;
 	reg = <0x9FF00000 0x20000>;
-	status = "okay";
+	status = "disabled";
 };
 
 &usbotg1 {
