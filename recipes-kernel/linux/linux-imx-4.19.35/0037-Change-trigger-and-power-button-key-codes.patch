From 185266fb89c5ecdecae76e1fb092f6fd8b07521c Mon Sep 17 00:00:00 2001
From: Erik Bengtsson <erik.bengtsson@flir.se>
Date: Tue, 18 Jun 2019 13:08:22 +0200
Subject: [PATCH] Change trigger and power button key codes

---
 arch/arm/boot/dts/imx7ulp-ec201.dtsi | 6 +++---
 1 file changed, 3 insertions(+), 3 deletions(-)

diff --git a/arch/arm/boot/dts/imx7ulp-ec201.dtsi b/arch/arm/boot/dts/imx7ulp-ec201.dtsi
index 88507d4f956e..32674de96894 100644
--- a/arch/arm/boot/dts/imx7ulp-ec201.dtsi
+++ b/arch/arm/boot/dts/imx7ulp-ec201.dtsi
@@ -74,7 +74,7 @@
 		TRIG {
 			label = "TRIG";
 			linux,input-type = <1>; /* EV_KEY */
-			linux,code = <0x120>;
+			linux,code = <KEY_F6>;
 			gpios = <&gpio_ptc 12 GPIO_ACTIVE_LOW>;
 			debounce-interval = <5>;
 		};
@@ -82,7 +82,7 @@
 		SW_ON {
 			label = "SW_ON";
 			linux,input-type = <1>; /* EV_KEY */
-			linux,code = <0x100>;
+			linux,code = <KEY_F8>;
 			gpios = <&gpio_ptc 13 GPIO_ACTIVE_LOW>;
 			debounce-interval = <5>;
 		};
@@ -308,7 +308,7 @@
 
 		onkey {
 			compatible = "fsl,pf1550-onkey";
-			linux,keycode = <KEY_POWER>;
+			linux,keycode = <KEY_F8>;
 			wakeup;
 		};
 
